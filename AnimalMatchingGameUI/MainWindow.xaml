<Window x:Class="AnimalMatchingGameUI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AnimalMatchingGameUI" xmlns:local1="clr-namespace:AnimalMatchingGame;assembly=AnimalMatchingGame"
        mc:Ignorable="d" FontSize="28"
        Title="Find all of the matching animals" Height="600" Width="500">
    <Window.Resources>
        <local1:Game x:Key="game"/>
    </Window.Resources>
    <Grid x:Name="Grid1" DataContext="{StaticResource game}" Margin="20">
        <Grid.ColumnDefinitions>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
        </Grid.RowDefinitions>
    </Grid>
</Window>
